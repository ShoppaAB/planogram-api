            "type": "string",
            "nullable": true
          },
          "startDate": {
            "type": "string",
            "format": "date",
            "nullable": true
          },
          "stopDate": {
            "type": "string",
            "format": "date",
            "nullable": true
          },
          "sourceVersion": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
