          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      },
      "delete": {
        "tags": ["PlanogramApiEndpoints"],
        "parameters": [
          {
            "name": "externalId",
            "in": "path",
